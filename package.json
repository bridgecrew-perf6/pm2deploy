    "@apicase/adapter-fetch": "^0.15.1",
    "@apicase/core": "^0.17.0",
    "@apicase/services": "^0.8.1",
    "@mdx-js/loader": "^1.5.7",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@tippy.js/react": "^3.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fingerprintjs2": "^1.8.6",
    "formik": "^2.1.4",
    "helmet": "^3.21.2",
    "history": "^4.9.0",
    "js-cookie": "^2.2.0",
    "mdx.macro": "^0.2.9",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-recaptcha": "^1.0.5",
    "react-lazyload": "^2.6.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.2.2",
    "tippy.js": "^5.2.0",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@svgr/cli": "^5.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.2",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.18.2",
    "react-is": "^16.13.0",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "lint:fix": "eslint --fix src",
    "lint:js": "eslint src",
    "lint:scss": "stylelint '**/*.scss' --config=.stylelintrc.json",
    "lint": "npm-run-all lint:*",
    "build-icon": "svgr --icon --replace-attr-values '#231f20=currentColor' --out-dir src/components/Icon/assets/build/ src/components/Icon/assets/src/",
    "build-js": "GENERATE_SOURCEMAP=false react-scripts build",
    "compress-image": "node compress-image.js",
    "convert-image": "node convert-image.js",
    "copy-to-deploy": "cpx 'build/**/*' build_deploy -C",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "start-js": "react-scripts start",
    "start": "npm run start-js",
    "start-secure": "HTTPS=true npm run start-js",
    "build": "npm run build-js && npm run copy-to-deploy && npm run compress-image",
    "deploy": "node server.js",
    "storybook": "start-storybook --docs -p 9009 -s public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile-js-unused": "react-scripts build",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,md}": [
      "eslint --fix src",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint '**/*.scss' --config=.stylelintrc.json --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
